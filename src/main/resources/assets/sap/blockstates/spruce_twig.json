{
    "multipart": [
        {   "apply": { "model": "sap:block/spruce_twig_center" }},
        {   "when": { "north": "true" },
            "apply": { "model": "sap:block/spruce_twig_north", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "sap:block/spruce_twig_east", "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "sap:block/spruce_twig_south", "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "sap:block/spruce_twig_west", "uvlock": true }
        },
        {   "when": { "up": "true" },
            "apply": { "model": "sap:block/spruce_twig_top", "uvlock": true }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "sap:block/spruce_twig_bottom", "uvlock": true }
        }
    ]
}
